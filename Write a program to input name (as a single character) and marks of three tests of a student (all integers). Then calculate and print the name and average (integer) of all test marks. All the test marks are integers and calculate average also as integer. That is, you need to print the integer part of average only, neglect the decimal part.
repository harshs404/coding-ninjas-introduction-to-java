import java.util.Scanner;
public class Solution {


	public static void main(String[] args)
    {
	int m1,m2,m3;
	Scanner s=new Scanner(System.in);
	String str=s.next();
	int m1=s.nextInt();
	int m2=s.nextInt();
	int m3=s.nextInt();
	int avg=(m1+m2+m3)/3;
	System.out.println(str);
	System.out.print(avg);
	}

}
